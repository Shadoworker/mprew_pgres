{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "code": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "stock": {
      "type": "integer"
    },
    "availability": {
      "type": "boolean",
      "default": true
    },
    "onDiscount": {
      "type": "boolean",
      "default": false
    },
    "discount": {
      "type": "integer"
    },
    "isActive": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "isCustomized": {
      "type": "boolean",
      "default": false
    },
    "description": {
      "type": "richtext"
    },
    "product_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-category.product-category",
      "inversedBy": "products"
    },
    "tag": {
      "type": "enumeration",
      "enum": [
        "HERO",
        "VEDETTES",
        "OTHERS"
      ]
    }
  }
}
